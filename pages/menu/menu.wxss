  width:100%;
  display:flex;
  flex-direction:row;
  justify-content: space-between;
  text-align: center;
}
.dish-item-pic{
  width:130rpx;
  height:130rpx;
  display:flex;
align-items:center;

}
.dish-item-pic image{
   width:130rpx;
  height:130rpx;
   margin: 0 auto
}
.dish-item-main{
  display: flex;
  flex-direction:column;
  justify-content: space-between;
}
.dish-item-info{
  /*width: 150rpx;*/
