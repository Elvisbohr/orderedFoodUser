/* pages/menu/menu.wxss */
page,.pages{
  height: 100%;
}
.pageCon{
  display: -webkit-flex;
  display: flex;
  flex-direction:column;
  
}
/*商铺头部*/
.menuHead{
  display: -webkit-flex;
  display: flex;
  height: 58px;
  align-items: center;
  padding: 0 30rpx;
  justify-content: space-between;
}
.menuHleft{
  display: -webkit-inline-flex;
  display: inline-flex;
  align-items: center;
}
.menuName{
  padding-left: 20rpx;
}
.menuImg{
  width: 48px;
}
.menuHright{
  width: 40rpx;
}
/*商铺头部End*/
/*店铺活动*/
.Lists{
  display: -webkit-flex;
  display: flex;
  align-items: center;
  justify-content:space-between;
  padding: 5px 30rpx;
  background: #fff;
}
.privilege,.prompt{
  padding-left:30rpx; 
}
.sImg{
  display: -webkit-flex;
  display: flex;
  font-size: 24rpx;
  height: 20px;
  align-items: center;
  color: #9a9a9a;
}
.sImg image{
  width: 30rpx;
  margin-right: 6rpx;
}
/*店铺活动End*/

/*选项卡*/
.weui-navbar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  z-index: 500;
  width: 100%;
}
.menutHead{
  width: 130rpx;
  color: #252525;
  font-size: 36rpx;
  text-align: center;
  height: 44px;
  line-height: 44px;
}
.menutHead.onTach{
  font-weight: bold;
  border-bottom: 2px solid #e23237;
}
.menuTabPanel {
  box-sizing: border-box;
  /*height: 100%;*/
  padding-top: 20rpx;
  /*overflow: auto;*/
  -webkit-overflow-scrolling: touch;
}
.pageTop{
  flex-shrink:0;
}

/*菜单样式*/
.image{
  width: 100%;
  height: 100%;
}
.index{
  height: 100%;
}
/*tab导航*/
.swiper-item{
  display: block;
  height: 300rpx;
}
.nav-tab-wrp{
  height: 88rpx;
  width: 100%;
  display: flex;
  flex-direction:row;
  background:rgba(250,250,250,1);
  position: fixed;
  top: -1rpx;
  z-index: 10
}
.nav-tab-item{
  width: 250rpx;
  text-align: center;
  line-height: 88rpx;
  color: rgb(102, 102, 102);
}
.nav-tab-item.active{
  border-bottom: 2rpx solid rgba(254,78,69,.8);
}
/*底部浮动*/
.cart-tab-wrp{
  height: 100rpx;
  position: fixed;
  bottom: 0rpx;
  background:#606060;
  display: flex;
  flex-direction:row;
  width: 100%;
  z-index: 10;
  color: black;
  /*display:none;*/
}
.cart-item-total{
  padding-left: 30rpx;
  width: 90%;
}
.cart-items{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top:-25rpx;
  background:#606060;
  border-radius:50rpx;
}
.cart-item-icon-wrp{
  width: 85rpx;
  height: 85rpx;
  position: absolute;
  top:14rpx;
  right: 8rpx;
  background:#ffac14;
  border-radius:45rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart-item-icon{
  width: 64rpx;
  height: 64rpx;

}
.cart-item-total-num{
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    font-size: 22rpx;
    color: white;
    line-height: 36rpx;
    border-radius: 36rpx;
    text-align: center;
    position: absolute;
    right: 2rpx;
    top: -10rpx;
    background: #f45449;
}
.cart-item-total-price{
    /*display: inline-block;*/
    width: 40rpx;
    height: 40rpx;
    font-size: 42rpx;
    color: #fff;
    line-height: 100rpx;
    text-align: center;
    padding-left: 110rpx;
}
.cart-item-yh-price{
  font-size: 24rpx;
  color: #fff;
  margin-left:10rpx; 

  
}
.cart-tab-item-btn{
    width: 160rpx;
    display: block;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 28rpx;
    border-left: 1px solid #fff;
    background: #e4333d;
    color: #fff;
    float: right;
    text-align: center;
}
.cart-tab-ok{
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
}
/*菜单wrap*/
.menu-wrp{
  overflow:hidden;
  width:750rpx;
  height: 100%;
  display:flex;
  background-color:white;
  flex-direction:row;
}

/*左侧菜单*/
.menu-item-wrp{
  text-align: center
}
.menu-item{
  width: 180rpx;
  height: 105rpx;
  color: #FFFFFF;
  display: flex;
  justify-content: center;
  align-items: center;
}
.menu-item.on{
  background-color: #fff;
}
.m-item-icon{
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  border-radius: 50%
}
.m-item-title{
  font-size: 24rpx;
  color: black
}
.left-side{
  height: 100%;
  display:flex;
  width: 200rpx;
  background-color: #f5f0ea;
  flex-direction:column;
}
/*右侧菜品*/
.dish-category-title{
  font-size: 28rpx;
  color: #888;
  border-bottommenushop:2rpx solid #ccc;
  padding: 20rpx 0;
  margin-bottom: 6rpx;
}
.right-side{
  width: 100%;
  height: 100%;
  color: #FFFFFF;
  justify-content: center;
  align-items: center;
}
