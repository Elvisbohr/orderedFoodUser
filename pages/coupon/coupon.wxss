/* pages/coupon/coupon.wxss */
.couponMain{
  padding: 20rpx 0;
}
.couList{
  background: #eee;
  position: relative;
  margin: 0 36rpx 20rpx;
}
.coupnBj{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 9;
}
.couL{
  display: -webkit-inline-flex;
  display: inline-flex;
  align-items: center;
  position: relative;
  z-index: 10;
}
.coulImg{
  width: 126rpx;
  height: 126rpx;
  margin: 30rpx;
}
.couShop{
  font-size: 24rpx;
  color: #fff
}
.couShop view{
  margin-bottom: 10rpx;
}
.couName{
  font-size: 32rpx;
}
.couR{
  width: 170rpx;
  text-align: center;
  flex-shrink: 0;
  color: #fff;
  font-size: 28rpx;
  position: relative;
  z-index: 10;
  height: 186rpx;
  display: -webkit-inline-flex;
  display: inline-flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-left: 1px dashed #eee
}
.coumsg{
  font-size: 28rpx;
}
.coumoney text{
  font-size: 68rpx;
}