/* 动画 */
  /* 箭头动画 */
 @-webkit-keyframes start {
	0%,30% {opacity: 0.4;-webkit-transform: translate(0,-10px);transform: translate(0,-10px);-webkit-transform:rotate(180deg);transform:rotate(180deg);}
	60% {opacity: 1;-webkit-transform: translate(0,0);transform: translate(0,0);-webkit-transform:rotate(180deg);transform:rotate(180deg);}
	100% {opacity: 0.4;-webkit-transform: translate(0,8px);transform: translate(0,8px);-webkit-transform:rotate(180deg);transform:rotate(180deg);}
} 
@keyframes start {
  0%,30% {opacity: 0.4;-webkit-transform: translate(0,-10px) rotate(180deg);transform: translate(0,-10px) rotate(180deg);}
    60% {opacity: 1;-webkit-transform: translate(0,0);transform: translate(0,0);-webkit-transform:rotate(180deg);transform:rotate(180deg);}
    100% {opacity: 0.4;-webkit-transform: translate(0,8px);transform: translate(0,8px);-webkit-transform:rotate(180deg);transform:rotate(180deg);}
}
 @-webkit-keyframes offend {
	0%,30% {opacity: 0;-webkit-transform: translate(0,-10px);}
	60% {opacity: 1;-webkit-transform: translate(0,0);}
	100% {opacity: 0;-webkit-transform: translate(0,8px);}
} 
@keyframes offend {
	0%,30% {opacity: 0;transform: translate(0,-10px);}
	60% {opacity: 1;transform: translate(0,0);}
	100% {opacity: 0;transform: translate(0,8px);}
} 